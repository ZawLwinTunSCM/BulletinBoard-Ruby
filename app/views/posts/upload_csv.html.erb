  <h3 class="text-center mt-5 mb-3">Upload CSV File</h3>
  <div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-4">
    <%= form_with url: import_csv_path do |form| %>
      <div class="custom-file mt-2">
        <%= form.label :file, class: "custom-file-label", id: "fileLabel" %>
        <%= form.file_field :file, class: "custom-file-input", accept: "text/csv" %>
      <%if notice %>
        <div class="error-msg"> <%= notice %> </div>
      <% end %>
      </div>
      <div class="text-center py-3">
        <%= form.submit "Upload", class: "btn btn-primary mr-2" %>
        <%= link_to "Back", posts_path, class: "btn btn-dark ml-2" %>
      </div>
    <% end %>
  </div>
  <div class="col-md-4"></div>
</div>